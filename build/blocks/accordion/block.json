{
  "apiVersion": 2,
  "name": "ab/accordions",
  "version": "1.0.0",
  "title": "Accessible Accordion",
  "category": "ab-block",
  "description": "Display content in accessible accordions",
  "supports": {
    "html": false,
    "anchor": true,
    "customClassName": true
  },
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "h2"
    }
  },
  "textdomain": "accordion-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}